<view>
    <view class="page">
    <view class="page-section page-section-spacing swiper">
      <swiper indicator-dots="{{indicatorDots}}"
              autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block wx:for="{{background}}" wx:key="*this">
          <swiper-item>
            <image src="{{item}}" class="swiper-item {{item}}"></image>
          </swiper-item>
        </block>
      </swiper>
    </view>
  </view>
 <view class="page-body">
        <input class="input"
               placeholder="请输入标题"
               value="{{itemVal}}"
               bindinput="onInputChange"
        />
  <button bindtap="getVal">提交</button>
  </view>
<view class="list" wx:for="{{list}}" wx:key="*this">
  <navigator url="../show/show?objectId={{item.id}}" class="title">
    {{index +1 }}: {{item.title}}
  </navigator>
</view>
</view>

