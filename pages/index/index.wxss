
.page {
    width: 100%;
    height: 100%;
}
.swiper-item{
    width: 100%;
    height: 100%;
}
.page-body{
    width: 100%;
    display: flex;
    justify-content: space-between;
    margin-top: 20px;
}
.page-body .input{
    width: 80%;
    font-size: 30rpx;
    /*background: hotpink;*/
    border: 1rpx solid #000;
}
.page-body button{
    flex-grow: 1;
    height: 50rpx;
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
    background: gray;
}
.list {
    border-bottom: 1rpx solid #999;
    padding: 20rpx 20rpx;
}

.title{
    line-height: 60rpx;
    font-size: 30rpx;
}


